{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "off",
        "no-unused-vars": "warn",
        "no-console": "off",
        "strict": ["error", "global"],
        "no-unexpected-multiline": "off",
        "no-use-before-define": "off"
    }
}
